{
  "name": "kidsloop-live-state",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "nodemon",
    "start:local": "ROUTE_PREFIX=/user ts-node src/main.dev.ts",
    "build": "webpack",
    "lint": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc ./**/*.{ts,test.ts} --write",
    "test:unit": "mocha --require ts-node/register \"tests/unit/**/*.test.ts\"",
    "test:integration": "mocha --require ts-node/register \"tests/integration/**/*.test.ts\" --timeout 8000",
    "test:acceptance": "DATABASE_URL=postgres://postgres:kidsloop@localhost/postgres1 mocha --require ts-node/register --file tests/testHelper.ts \"tests/acceptance/**/*.test.ts\" --timeout 8000",
    "test": "npm run test:unit && npm run test:integration",
    "coverage": "nyc -r lcov -e .ts -x tests --reporter=json-summary npm run test",
    "make-badges": "istanbul-badges-readme",
    "test:file": "mocha --require ts-node/register --file tests/testHelper.ts tests/integration/directives/isMIMEType.test.ts --timeout 5000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "immutable": "*",
    "kidsloop-live-serialization": "file:../serialization",
    "redux": "^4.1.1",
    "redux-immutable": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/chai-as-promised": "7.1.3",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/faker": "5.1.4",
    "@types/mocha": "8.0.3",
    "@types/sinon-chai": "3.2.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-match": "^1.1.1",
    "deep-equal-in-any-order": "^1.1.8",
    "eslint": "7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "^2.24.0",
    "faker": "5.1.0",
    "istanbul-badges-readme": "1.0.5",
    "js-yaml": "^3.14.1",
    "mocha": "8.2.1",
    "node-mocks-http": "1.9.0",
    "nodemon": "2.0.6",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "supertest": "^6.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "uuid": "^3.4.0"
  }
}
